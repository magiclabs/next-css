{
  "name": "@magiclabs/next-css",
  "version": "0.1.0",
  "description": "Improved CSS support in NextJS with flexiblity to add any pre-processor you like",
  "author": "Magic Labs <open-source@magic.link>",
  "license": "MIT",
  "repository": "magiclabs/better-next-css",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "yarn clean && ncc build -w src/index.ts -o dist",
    "build": "yarn clean && ncc build src/index.ts -o dist",
    "lint": "eslint --fix src/**/*.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@ikscodes/eslint-config": "^7.0.2",
    "@ikscodes/prettier-config": "^2.0.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^7.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "next": "^10.0.0"
  },
  "auto": {
    "plugins": [
      "npm"
    ]
  }
}
