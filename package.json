{
  "name": "@magiclabs/next-css",
  "version": "0.2.2",
  "description": "Feature-parity with NextJS's built-in CSS with the flexibility to support your favorite pre-processor(s)!",
  "author": "Magic Labs <open-source@magic.link>",
  "license": "MIT",
  "repository": "magiclabs/better-next-css",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "yarn clean && tsc -w",
    "build": "yarn clean && tsc",
    "lint": "eslint --fix src/**/*.ts",
    "test": "echo \"No unit tests... yet :(\" && exit 0",
    "clean": "rimraf dist",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "browserslist": "^4.19.1",
    "chalk": "^4.1.0",
    "css-loader": "6.5.1",
    "css-minimizer-webpack-plugin": "3.3.1",
    "file-loader": "6.2.0",
    "findup-sync": "^4.0.0",
    "ignore-loader": "0.1.2",
    "json5": "^2.2.0",
    "lodash.uniq": "^4.5.0",
    "mini-css-extract-plugin": "2.4.5",
    "postcss": "8.4.5",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.1.0",
    "wrap-ansi": "^7.0.0",
    "line-column": "1.0.2"
  },
  "devDependencies": {
    "@ikscodes/eslint-config": "^7.0.2",
    "@ikscodes/prettier-config": "^2.0.1",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/findup-sync": "^2.0.2",
    "@types/lodash.uniq": "^4.5.6",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.31",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "auto": "^10.16.8",
    "eslint": "^7.14.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "next": "^12.0.7",
    "pinst": "^2.1.6",
    "prettier": "^2.2.0",
    "typescript": "^4.2.2",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "next": "^9.5.4 || ^10.0.0"
  },
  "auto": {
    "plugins": [
      "npm"
    ]
  }
}
